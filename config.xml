<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.helloworld" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Educação Ambiental</name>
    <description>
        Aplicativo oficial do Portal de Educação Ambiental de Santa Catarina.
    </description>
    <author email="caue1017@edu.univali.br" href="http://github.com/dcaueb78">
        Haune Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http:///" />
    <allow-intent href="https:///" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
</widget>
